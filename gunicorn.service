[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=parallels
Group=parallels
WorkingDirectory=/home/parallels/TurtleMaster
ExecStart=/home/parallels/TurtleMaster/venv37/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/parallels/TurtleMaster/TurtleMaster.sock TurtleMaster.wsgi:application

[Install]
WantedBy=multi-user.target
